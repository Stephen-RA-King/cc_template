{
  "_copy_without_render": [
    ".github/workflows/*"
    ],
  "_extensions": [
    "jinja2_time.TimeExtension"
  ],
  "project_name": "Python Boilerplate",
  "project_slug": "{{ cookiecutter.project_name.lower()|replace(' ', '_')|replace('-', '_')|replace('.', '_')|trim() }}",
  "pkg_name": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
  "project_short_description": "boilerplate you need to create a Python package.",
  "author_name": "Stephen R A King",
  "email": "stephen.ra.king@gmail.com",
  "github_username": "Stephen-RA-King",
  "initial_git_branch_name": [
    "main",
    "master",
    "trunk",
    "development"
  ],
  "development_OS": [
    "windows",
    "linux"
  ],
  "target_OS": [
    "all",
    "windows",
    "linux"
  ],
  "create_author_file": "y",
  "version": "0.1.0",
  "_year": "{% now 'utc', '%Y' %}",
  "use_docker": "y",
  "use_pytest": "y",
  "use_black": "y",
  "use_flake8": "y",
  "use_mypy": "y",
  "use_commitizen": "y",
  "version_control": [
    "python_semantic_release",
    "bump2version"
  ],
  "docstrings_style": [
    "numpy",
    "google"
  ],
  "config_file": [
    "all",
    "yaml",
    "json",
    "ini",
    "toml",
    "none"
  ],
  "resource_file": [
    "all",
    "pickle",
    "png",
    "db",
    "none"
  ],
  "use_isort": "y",
  "use_sphinx": "y",
  "use_pre_commit": "y",
  "use_cruft": "y",
  "use_bandit": "y",
  "use_safety": "y",
  "use_logging": "y",
  "include_example_scripts": "y",
  "command_line_interface": [
    "Click",
    "No command-line interface"
  ],
  "license": [
    "MIT",
    "BSD-3-Clause",
    "ISC",
    "Apache-2.0",
    "GPL-3.0-only",
    "Not open source"
  ]
}
